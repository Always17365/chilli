<scm>
  <datamodel>
    <data id="dialNumber" expr=""></data>
  </datamodel>
  <functionmodel>
	<function >function log(var1){logging(var1);}</function>
  </functionmodel>

	<state id="init">
		<onentry>
			<log level = "info" >enter Init state.</log>
		</onentry>
        <event event="event1" >
			<transition  target="state1"/>
		</event>
		<onexit>
			<log level = "info" >exit Init state.</log>
		</onexit>
		<state id="state1">
			<onentry>
				<log level = "info">enter state1 state.</log>
				<log level = "info" type="script"> new Date();</log>
				<send target="testsend" type="cmd" idlocation="extension.number;" from="extension.number;" dest="extension.number;" ></send>
			</onentry>
			<event event="init">
				<transition  target="init"/>
			</event>
		</state>

    </state>
</scm>

